<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="app.ishizaki.ryu.examapplication">

    <application
        android:name=".RealmApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ExamApplication">
        <activity
            android:name=".AddTimeTableActivity"
            android:exported="false" />
        <activity
            android:name=".startActivity"
            android:exported="true"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AddCoverageActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name=".AddToDoActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
    </application>

</manifest>