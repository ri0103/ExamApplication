<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/background_main"
    tools:context=".ResultActivity">


    <TextView
        android:id="@+id/warningBetaText"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="10sp"
        android:gravity="center"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        android:text="注：こちらの機能は開発段階のため不具合があるかもしれません(T_T)"
        android:layout_marginTop="4sp"
        />

    <EditText
        android:layout_height="wrap_content"
        android:layout_width="320sp"
        android:layout_margin="20sp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toTopOf="@id/calculateResultTextViews"
        android:gravity="center"
        android:layout_marginTop="10sp"
        android:fontFamily="@font/mplusbold"
        android:id="@+id/examNameResultET"
        android:hint="試験名"
        android:textSize="36sp"
        />





    <TableLayout
        android:id="@+id/calculateResultTextViews"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toTopOf="@id/tableResultScores"
        app:layout_constraintTop_toBottomOf="@id/examNameResultET"
        android:layout_marginVertical="18sp">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="10sp">

            <TextView
                android:layout_height="wrap_content"
                android:layout_width="0dp"
                android:layout_weight="2"
                android:textSize="30sp"
                android:paddingLeft="30sp"
                android:text="合計"
                android:fontFamily="@font/mplusbold"
                />

            <TextView
                android:id="@+id/myScoreResultSum"
                android:layout_height="wrap_content"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:textSize="30sp"
                android:textAlignment="center"
                android:fontFamily="@font/mplusbold"
                android:text="0"
                />
            <TextView
                android:id="@+id/averageScoreResultSum"
                android:layout_height="wrap_content"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:textSize="30sp"
                android:textAlignment="center"
                android:text="0"
                android:fontFamily="@font/mplusbold"
                />

        </TableRow>

    </TableLayout>


    <TableLayout
        android:id="@+id/tableResultScores"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="24sp"
        android:layout_marginHorizontal="10sp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toTopOf="@id/saveResultButton"
        app:layout_constraintTop_toBottomOf="@id/calculateResultTextViews">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="2"
            android:layout_marginBottom="20sp"
            >
            <TextView
                android:layout_height="60sp"
                android:gravity="center_vertical"
                android:layout_width="0dp"
                android:layout_weight="2"
                android:text="科目名"
                android:paddingLeft="20sp"
                android:textSize="22sp"
                android:background="@drawable/border1"
                android:fontFamily="@font/mplusmedium"

                />
            <TextView
                android:layout_height="60sp"
                android:gravity="center"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:text="点数"
                android:textSize="22sp"
                android:background="@drawable/border1"
                android:fontFamily="@font/mplusmedium"
                />
            <TextView
                android:layout_height="60sp"
                android:gravity="center"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:text="平均点"
                android:textSize="22sp"
                android:background="@drawable/border1"
                android:fontFamily="@font/mplusmedium"
                />
        </TableRow>


